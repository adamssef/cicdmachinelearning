{
  "name": "planet/planet_platform",
  "type": "project",
  "description": "Planet",
  "license": "GPL-2.0-or-later",
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "acquia/cohesion": "6.9.0",
    "acquia/cohesion-theme": "6.9.0",
    "composer/installers": "^1.2",
    "cweagans/composer-patches": "^1.6",
    "d3/d3": "^3.5",
    "drupal/acquia_connector": "^3.0",
    "drupal/admin_toolbar": "^3.1",
    "drupal/autologout": "^1.4",
    "drupal/color-color": "^1.0",
    "drupal/config_filter": "^1.10",
    "drupal/config_ignore": "^2.3",
    "drupal/config_split": "^1.9",
    "drupal/config_split_ignore": "^1.9",
    "drupal/core-composer-scaffold": "^9.0.0",
    "drupal/core-project-message": "^9.0.0",
    "drupal/core-recommended": "^9.0.0",
    "drupal/csp": "^1.16",
    "drupal/devel": "^4.1",
    "drupal/dynamic_entity_reference": "^1.16",
    "drupal/entity_language_fallback": "^1.4",
    "drupal/environment_indicator": "^4.0",
    "drupal/flood_control": "^2.2",
    "drupal/google_tag": "^1.5",
    "drupal/hal-hal": "^1.0",
    "drupal/jsonapi_extras": "^3.21",
    "drupal/login_security": "^2.0",
    "drupal/memcache": "^2.4",
    "drupal/metatag": "^1.21",
    "drupal/pardot": "^2.0@beta",
    "drupal/password_policy": "^3.1",
    "drupal/recaptcha_v3": "^1.6",
    "drupal/robotstxt": "^1.4",
    "drupal/search_api_solr": "^4.2",
    "drupal/shield": "^1.6",
    "drupal/simple_sitemap": "^4.1",
    "drupal/site_studio_webform": "^1.1",
    "drupal/views_autosubmit": "^1.4",
    "drupal/wb_countries": "1.0.x-dev#dcfbad6754d04f42adce49f7f9f89b5010fe4be8",
    "drupal/webform": "^6.1",
    "drupal/webform_pardot": "^2.0",
    "drush/drush": "^10.0.0",
    "erusev/parsedown": "^1.7",
    "highlightjs/highlightjs": "^9.7",
    "mglaman/phpstan-drupal": "0.12.12",
    "oomphinc/composer-installers-extender": "^2.0",
    "sainsburys/guzzle-oauth2-plugin": "^3.0",
    "vlucas/phpdotenv": "^2.4",
    "webflo/drupal-finder": "^1.0.0",
    "webmozart/path-util": "^2.3",
    "zaporylie/composer-drupal-optimizations": "^1.0"
  },
  "require-dev": {
    "dg/bypass-finals": "^1.3",
    "drupal/console": "^1.9",
    "drupal/core-dev": "^9.0.0",
    "kint-php/kint": "^3.3",
    "phpspec/prophecy-phpunit": "^2"
  },
  "config": {
    "preferred-install": {
      "*": "auto"
    },
    "discard-changes": true,
    "sort-packages": true,
    "allow-plugins": {
      "cweagans/composer-patches": true,
      "composer/installers": true,
      "drupal/core-composer-scaffold": true,
      "drupal/core-project-message": true,
      "zaporylie/composer-drupal-optimizations": true,
      "drupal/console-extend-plugin": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "oomphinc/composer-installers-extender": true
    }
  },
  "extra": {
    "drupal-scaffold": {
      "file-mapping": {
        "[web-root]/.htaccess": false,
        "[web-root]/sites/development.services.yml": false
      },
      "locations": {
        "web-root": "docroot/"
      }
    },
    "enable-patching": true,
    "installer-paths": {
      "docroot/core": [
        "type:drupal-core"
      ],
      "docroot/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "docroot/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "docroot/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "docroot/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "docroot/modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "docroot/themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "patches": {
      "drupal/core": {
        "Invalid config structures can result in exceptions when saving a config entity": "https://www.drupal.org/files/issues/2021-11-29/2925890-29.patch",
        "https://www.drupal.org/project/drupal/issues/3091336": "https://www.drupal.org/files/issues/2021-01-05/AliasRepository-proper-language-fallback-3091336-62.patch",
        "Fix alias fallback": "patches/path_auto_alias_fallback.patch"
      },
      "drupal/config_ignore": {
        "Support for export filtering via Drush": "https://www.drupal.org/files/issues/2021-01-04/config_ignore_2857247-61.patch"
      },
      "drupal/pardot": {
        "Disable pardot js": "patches/path_pardot.patch"
      }
    }
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    "d3": {
      "type": "package",
      "package": {
          "name": "d3/d3",
          "version": "v3.5.17",
          "type": "drupal-library",
          "source": {
              "url": "https://github.com/d3/d3.git",
              "type": "git",
              "reference": "tags/v3.5.17"
          }
      }
    },
    "highlighjs": {
      "type": "package",
      "package": {
          "name": "highlightjs/highlightjs",
          "version": "9.7.0",
          "type": "drupal-library",
          "source": {
              "url": "https://github.com/highlightjs/highlight.js.git",
              "type": "git",
              "reference": "tags/9.7.0"
          }
      }
    }
  },
  "scripts": {
    "post-install-cmd": [
      "test -e docroot/robots.txt && rm docroot/robots.txt || echo robots already deleted"
    ],
    "post-update-cmd": [
      "test -e docroot/robots.txt && rm docroot/robots.txt || echo robots already deleted"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
