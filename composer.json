{
  "name": "planet/planet_platform",
  "type": "project",
  "description": "Planet",
  "license": "GPL-2.0-or-later",
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "acquia/cohesion": "^7.4",
    "acquia/cohesion-theme": "^7.4",
    "composer/installers": "^2.2",
    "cweagans/composer-patches": "^1.7",
    "d3/d3": "^3.5",
    "drupal/acquia_analytics_redirects": "^1.0",
    "drupal/acquia_connector": "^4.0",
    "drupal/admin_toolbar": "^3.4",
    "drupal/animated_gif": "^2.0",
    "drupal/asset_injector": "^2.19",
    "drupal/autologout": "^1.4",
    "drupal/block_class": "^4.0",
    "drupal/ccos": "^2.1",
    "drupal/chatgpt_plugin": "^2.1",
    "drupal/clientside_validation": "^4.0",
    "drupal/color": "^1.0",
    "drupal/config_filter": "^2.6",
    "drupal/config_ignore": "^3.2",
    "drupal/config_split": "^2.0",
    "drupal/content_lock": "^2.3",
    "drupal/content_moderation_edit_notify": "^2.0",
    "drupal/context": "^5.0@RC",
    "drupal/core-composer-scaffold": "^10.2",
    "drupal/core-project-message": "^10.2",
    "drupal/core-recommended": "dev-default",
    "drupal/csp": "^1.30",
    "drupal/ctools": "^4.0",
    "drupal/devel": "^5.1",
    "drupal/duplicate_role": "^1.3",
    "drupal/dynamic_entity_reference": "^3.1",
    "drupal/encrypt": "^3.1",
    "drupal/entity_clone": "^2.0@beta",
    "drupal/entity_language_fallback": "^1.5",
    "drupal/environment_indicator": "^4.0",
    "drupal/facets": "^3.0@beta",
    "drupal/field_group": "^3.4",
    "drupal/flood_control": "^2.3",
    "drupal/google_analytics": "^4.0",
    "drupal/google_tag": "^2.0",
    "drupal/gtm": "^1.8",
    "drupal/hal-hal": "^1.0",
    "drupal/hidden_language": "^2.0@beta",
    "drupal/honeypot": "^2.1",
    "drupal/jsonapi_extras": "^3.24",
    "drupal/login_security": "^2.0",
    "drupal/media_entity_file_replace": "^1.2",
    "drupal/media_entity_lottie": "^2.1",
    "drupal/media_thumbnail_url_formatter": "^3.0",
    "drupal/menu_item_extras": "^3.0",
    "drupal/menu_link_attributes": "^1.3",
    "drupal/metatag": "^2.0",
    "drupal/migrate_source_csv": "^3.6",
    "drupal/modal_page": "^5.0",
    "drupal/modules_weight": "^3.0",
    "drupal/node_revision_delete": "^1.0@RC",
    "drupal/page_cache_query_ignore": "^2.2",
    "drupal/paragraphs": "^1.18",
    "drupal/pardot": "^2.0@beta",
    "drupal/password_policy": "^4.0",
    "drupal/pathauto": "^1.12",
    "drupal/quick_node_clone": "^1.22",
    "drupal/rdf": "^2.1",
    "drupal/real_aes": "^2.5",
    "drupal/recaptcha": "^3.2",
    "drupal/recaptcha_v3": "^2.0",
    "drupal/redirect": "^1.9",
    "drupal/redis": "^1.8",
    "drupal/robotstxt": "^1.5",
    "drupal/salesforce": "^5.0",
    "drupal/schema_metatag": "^3.0",
    "drupal/search_api_solr": "^4.3",
    "drupal/sfweb2lead_webform": "^6.0",
    "drupal/shield": "^1.7",
    "drupal/simple_sitemap": "^4.1",
    "drupal/simplelogin": "^7.0",
    "drupal/site_settings": "^1.20",
    "drupal/site_studio_webform": "^1.1",
    "drupal/smtp": "^1.2",
    "drupal/structure_sync": "^2.0",
    "drupal/svg_image_field": "^2.3",
    "drupal/tfa": "^1.5",
    "drupal/tmgmt": "^1.15",
    "drupal/tmgmt_transifex": "^2.0",
    "drupal/toolbar_edit_page_button": "^1.0",
    "drupal/trash": "^3.0",
    "drupal/ultimate_cron": "^2.0@alpha",
    "drupal/verf": "^2.0",
    "drupal/views_autosubmit": "^1.6",
    "drupal/views_fieldsets": "^4.0",
    "drupal/views_filters_populate": "^2.1",
    "drupal/views_rows_wrapper": "^2.1",
    "drupal/webform": "^6.2",
    "drupal/webform_pardot": "^2.0@beta",
    "drush/drush": "^12.4",
    "erusev/parsedown": "^1.7",
    "guzzlehttp/guzzle": "^7.8",
    "highlightjs/highlightjs": "^9.7",
    "mglaman/composer-drupal-lenient": "^1.0",
    "npm-asset/jquery-validation": "^1.20",
    "oomphinc/composer-installers-extender": "^2.0",
    "phpstan/phpstan": "^1.10",
    "sainsburys/guzzle-oauth2-plugin": "^3.0",
    "vlucas/phpdotenv": "^5.6",
    "webflo/drupal-finder": "^1.2",
    "webmozart/path-util": "^2.3",
    "zaporylie/composer-drupal-optimizations": "^1.2"
  },
  "require-dev": {
    "dg/bypass-finals": "^1.3",
    "drupal/core-dev": "^10",
    "kint-php/kint": "^3.3",
    "mglaman/phpstan-drupal": "^1.2",
    "phpspec/prophecy-phpunit": "^2"
  },
  "config": {
    "preferred-install": {
      "*": "auto"
    },
    "discard-changes": true,
    "sort-packages": true,
    "allow-plugins": {
      "cweagans/composer-patches": true,
      "composer/installers": true,
      "drupal/core-composer-scaffold": true,
      "drupal/core-project-message": true,
      "zaporylie/composer-drupal-optimizations": true,
      "drupal/console-extend-plugin": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "oomphinc/composer-installers-extender": true,
      "mglaman/composer-drupal-lenient": true,
      "php-http/discovery": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "drupal-scaffold": {
      "file-mapping": {
        "[web-root]/.htaccess": false,
        "[web-root]/sites/development.services.yml": false
      },
      "locations": {
        "web-root": "docroot/"
      }
    },
    "enable-patching": true,
    "installer-paths": {
      "docroot/core": [
        "type:drupal-core"
      ],
      "docroot/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "docroot/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "docroot/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "docroot/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "docroot/modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "docroot/themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "patches": {
      "drupal/core": {
        "Invalid config structures can result in exceptions when saving a config entity": "https://www.drupal.org/files/issues/2021-11-29/2925890-29.patch",
        "https://www.drupal.org/project/drupal/issues/3091336": "https://www.drupal.org/files/issues/2021-01-05/AliasRepository-proper-language-fallback-3091336-62.patch",
        "Deprecated function: mb_strtolower(): Passing null to parameter #1 ($string)": "patches/core_condition.patch",
        "Invalid translation language (it) specified": "patches/core_contentedit.patch",
        "https://www.drupal.org/project/drupal/issues/3006897": "https://www.drupal.org/files/issues/2022-11-15/3006897-17.patch",
        "Fix invalid alias": "patches/translations_invalid_path.patch",
        "Remove unexisting permissions": "https://www.drupal.org/files/issues/2023-09-14/3358586-11.patch",
        "'Output is empty' when running drush cohesion:rebuild": "https://www.drupal.org/files/issues/2023-09-13/3386915-6.patch",
        "An empty views pager offset field can cause a PHP type error to be triggered": "https://www.drupal.org/files/issues/2023-05-30/3361177-8.patch"
        
      },
      "drupal/pardot": {
        "Disable pardot js": "patches/path_pardot.patch",
        "https://www.drupal.org/files/issues/2024-01-04/3382957-pardot-D10-compatibility.patch": "https://www.drupal.org/files/issues/2024-01-04/3382957-pardot-D10-compatibility.patch",
        "Fix dynamic creation of a property": "patches/pardot.patch"
      },
      "drupal/autologout": {
        "Undefined index destination": "https://www.drupal.org/files/issues/2022-07-28/3300118-undefined-index-destination.patch"
      },
      "acquia/cohesion": {
        "Media alt and name": "patches/cohesion_690.patch",
        "Planet font-style": "patches/cohesion_690_2.patch",
        "API patch": "patches/cohesion_api.patch",
        "API assets patch": "patches/cohesion_api_assets_fix.patch"
      },
      "drupal/webform_pardot": {
        "Custom pardot for planet": "patches/webform_pardot.patch"
      },
      "drupal/redirect": {
        "Remove trailing slash": "patches/redirect_domain.patch"
      },
      "drupal/entity_language_fallback": {
        "Deprecated function: Creation of dynamic property": "https://www.drupal.org/files/issues/2023-12-11/issue-3407832-srcfallbackcontroller.php-error-on-fixes.patch"
      },
      "drupal/config_split_ignore": {
        "IgnoreFilter class not found": "https://www.drupal.org/files/issues/2024-01-10/ignore-filter-not-found-3413717-3.patch"
      },
      "drupal/salesforce": {
        "Creation of dynamic property time": "https://www.drupal.org/files/issues/2023-10-27/3397134-2.patch"
      },
      "drupal/ultimate_cron": {
        "Creation of dynamic property": "https://www.drupal.org/files/issues/2023-04-03/3351276-4.patch"
      },
      "drupal/site_settings": {
        "Site Settings Menu does not work in 10.2.1": "https://www.drupal.org/files/issues/2024-01-17/3357022_canonical_removed_from_type.patch"
      },
      "drupal/dynamic_entity_reference": {
        "Test": "https://www.drupal.org/files/issues/2023-10-24/3396393-2.patch"
      },
      "drupal/simple_sitemap": {
        "edit table": "patches/simple_sitemap.patch"
      },
      "drupal/redis": {
        "Change the TTL": "patches/redis-file.patch"
      }
    },
    "drupal-lenient": {
      "allowed-list": ["drupal/pardot"]
    }
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    "d3": {
      "type": "package",
      "package": {
        "name": "d3/d3",
        "version": "v3.5.17",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/d3/d3.git",
          "type": "git",
          "reference": "tags/v3.5.17"
        }
      }
    },
    "highlighjs": {
      "type": "package",
      "package": {
        "name": "highlightjs/highlightjs",
        "version": "9.7.0",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/highlightjs/highlight.js.git",
          "type": "git",
          "reference": "tags/9.7.0"
        }
      }
    },
    "dropzone": {
      "type": "package",
      "package": {
        "name": "enyo/dropzone",
        "version": "4.2.0",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/enyo/dropzone",
          "type": "git",
          "reference": "origin/master"
        }
      }
    },
    "npm-assets": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "test -e docroot/robots.txt && rm docroot/robots.txt || echo robots already deleted"
    ],
    "post-update-cmd": [
      "test -e docroot/robots.txt && rm docroot/robots.txt || echo robots already deleted"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
