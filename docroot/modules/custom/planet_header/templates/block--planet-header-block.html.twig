{% set data = elements.content[0]['#data'] %}
{% if data['#current_language_prefix'] == '' %}
  {% set data = data|merge({'#current_language_prefix': 'en'}) %}
{% endif %}

<div id="page-loader"
     role="status"
     aria-live="polite"
     aria-label="Loading page"
     style="inset: 0; display: flex; align-items: center; justify-content: center; height: 100%; z-index: 99999999; position: fixed; background: white;">
  <img src="/resources/icons/loader.svg"
       alt=""
       aria-hidden="true"
       style="max-width: 80px; max-height: 80px; text-align: center;">
  <span class="sr-only" style="position: absolute; width: 1px; height: 1px; padding: 0; overflow: hidden; clip: rect(0, 0, 0, 0); white-space: nowrap; border: 0;">
    Loading page
  </span>
</div>
<div class="megamenu-complex-container">
  <header class='megamenu-header'>
    <div class="inside-header-container">
      {% if data['#current_language_prefix'] == '' or data['#current_language_prefix'] == 'en' %}
      <a class="megamenu-link go-home" href="/">
        {% elseif data['#current_language_prefix'] == 'it' %}
        <a class="megamenu-link go-home" href="/it">
          {% elseif data['#current_language_prefix'] == 'fr' %}
          <a class="megamenu-link go-home" href="/fr">
            {% elseif data['#current_language_prefix'] == 'de' %}
            <a class="megamenu-link go-home" href="/de">
              {% elseif data['#current_language_prefix'] == 'es' %}
              <a class="megamenu-link go-home" href="/es">
                {% endif %}
                <img width="122" height="27" id="planet-logo" src="/resources/logo/planet_logo.svg" alt="Planet Logo" class="planet-logo">
              </a>
              <div class="header-menus">
                <nav class="header-menu main-menu">
                  {% for item in data['megamenu'] %}
                    <span id="{{ item['id'] }}" class="business-li megamenu-link">
                 <a {% if item['_blank'] %}target="_blank" {% endif %}class="megamenu-link business-link" href="javascript:void(0);">{{ item['title']|t }}
                  <img id="{{ item['id'] }}_img" src="/resources/icons/arrow-down.svg" alt="Icon pointing down"/>
                 </a>
               </span>
                  {% endfor %}
                </nav>
                <nav class="header-menu secondary-planet-menu">
                  {% for item in data['megamenu_cta'] %}
                    <span class="{{ item['classes'] }}">
                      <a class="megamenu-link" {% if item['_blank'] %}target="_blank" {% endif %} href="{% if item['url'][data['#current_language_prefix']] %}{% if item['url'][data['#current_language_prefix']] %}{{ item['url'][data['#current_language_prefix']] }}{% else %}{{ item['url']['en'] }}{% endif %}{% else %}{{ item['url'] }}{% endif %}">{{ item['title']|t }}</a>
                    </span>
                  {% endfor %}
                </nav>
              </div>
    </div>
    <div class="inside-header-container--mobile-and-tablet">
      {% if data['#current_language_prefix'] == '' or data['#current_language_prefix'] == 'en' %}
      <a class="megamenu-link go-home" href="/">
        {% elseif data['#current_language_prefix'] == 'it' %}
        <a class="megamenu-link go-home" href="/it">
          {% elseif data['#current_language_prefix'] == 'fr' %}
          <a class="megamenu-link go-home" href="/fr">
            {% elseif data['#current_language_prefix'] == 'de' %}
            <a class="megamenu-link go-home" href="/de">
              {% elseif data['#current_language_prefix'] == 'es' %}
              <a class="megamenu-link go-home" href="/es">
                {% endif %}
                <img id="planet-logo--mobile-and-tablet" src="/resources/logo/planet_logo.svg" alt="Planet Logo" class="planet-logo">
              </a>
              <span class="go-back-span display-none--desktop display-none">
         <a class="megamenu-link go-back-to-first-level display-none--desktop" href="#">
         <img src="/resources/icons/arrow-left.svg" alt="Go back icon" class="arrow-left">
         </a>
         {{ "Back"|t }}
         </span>
              <img src="/resources/icons/hamburger-menu.svg" alt="Hamburger Menu" class="hamburger-menu">
              <img src="/resources/icons/closing-x.svg" alt="Close menu icon." class="close-hamburger-menu display-none rotate-right">
    </div>
  </header>
