        </div>
      {% endfor %}
      <span class="megamenu-column__title megamenu-column__title--mt32px">{{ "Software for retail"|t }}</span>
      {% for item in data['megamenu']['0']['children']['software_for_retail'] %}
        <div class="megamenu-column__item">
          <a class="megamenu-link" {% if item['_blank'] %}target="_blank" {% endif %}href="{% if item['url'][data['#current_language_prefix']] %}{{ item['url'][data['#current_language_prefix']] }}{% else %}{{ item['url']['en'] }}{% endif %}">
            <img class="megamenu-column__icon megamenu-column__icon__arrow" src="{{ item['icon_path'] }}"/>
