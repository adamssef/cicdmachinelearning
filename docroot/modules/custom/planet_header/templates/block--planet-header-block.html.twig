            <div class="megamenu-column__mini-text">{{ item['description']|t }}</div>
          </a>
          {% endif %}
        </div>
      {% endfor %}
      <div class="navigation-item"></div>
      <div class="navigation-item"></div>
      <div class="navigation-item"></div>
    </div>
    <div class="last-four-case-studies">
      <div class="megamenu-column__title">{{ "Featured customer stories"|t }}</div>
      <div class="iteration-container">
        {% for case_study in data.case_studies %}
          <a class="megamenu-link" href="{{ case_study.url }}">
            <div class="last-four-case-studies__case_study">
              <div class="four-last-case-studies__item" style="background: url('{{ case_study.image_url }}'); background-size: cover; background-repeat: no-repeat; background-position: center;"></div>
              <span class="megamenu-column__mini-header">{{ case_study.title }}</span>
            </div>
          </a>
        {% endfor %}
      </div>
    </div>
  </div>
  <div class="megamenu-company__desktop display-none">
    <div class="navigation-items">
      {% for item in data['megamenu']['3']['children'] %}
        <div class="{{ item['classes'] }}">
          <a class="megamenu-link" {% if item['_blank'] %}target="_blank" {% endif %}href="{% if item['url'][data['#current_language_prefix']] %}{{ item['url'][data['#current_language_prefix']] }}{% else %}{{ item['url']['en'] }}{% endif %}">
