            <div class="megamenu-column__mini-text">{{ item['description']|t }}</div>
          </a>
        </div>
      {% endfor %}
    </div>
    <div class="last-four-case-studies">
      <div class="megamenu-column__title">{{ "Featured customer stories"|t }}</div>
      <div class="iteration-container">
        {% set case_study_count = data.case_studies|length - 1 %}
        {% for key, case_study in data.case_studies %}
          <a class="megamenu-link" href="{{ case_study.url }}">
            <div class="last-four-case-studies__case_study">
              <div class="four-last-case-studies__item four-last-case-studies__item--last" style="background: url('{{ case_study.image_url }}'); background-size: cover; background-repeat: no-repeat; background-position: center;"></div>
              {% if key == case_study_count %}
                <span class="megamenu-column__mini-header megamenu-column__mini-header--last">{{ case_study.title }}</span>
              {% else %}
                <span class="megamenu-column__mini-header">{{ case_study.title }}</span>
              {% endif %}
            </div>
          </a>
        {% endfor %}
      </div>
    </div>
  </div>
  <div class="megamenu-mobile-and-tablets__container megamenu-mobile-and-tablets__container--company display-none">
    <div class="navigation-items">
      {% for item in data['megamenu']['3']['children'] %}
        <div class="{{ item['classes'] }}">
          <a class="megamenu-link" {% if item['_blank'] %}target="_blank" {% endif %}href="{% if item['url'][data['#current_language_prefix']] %}{{ item['url'][data['#current_language_prefix']] }}{% else %}{{ item['url']['en'] }}{% endif %}">
            <div class="megamenu-column__mini-header">{{ item['title']|t }}</div>
            <div class="megamenu-column__mini-text">{{ item['description']|t }}</div>
          </a>
        </div>
      {% endfor %}
    </div>
  </div>
</div>
